<%- include("_header") -%>

  <h1>Insert Product</h1>

  <form action="/insert" method="post">
    <p>ID: <input type="text" name="prod_id" required></p>
    <p>Name: <input type="text" name="prod_name" required></p>
    <p>Description: <input type="text" name="prod_desc"></p>
    <p>Price: <input type="text" name="prod_price" required></p>
    <input type="submit" value="Insert Product">
  </form>

